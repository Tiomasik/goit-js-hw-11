<div class="photo-card">
    <img class="img-card" src="{{webformatURL}}" alt="{{tags}}" loading="lazy" />
    <div class="info">
        <p class="info-item">
            <b>Likes </b>
            <span>{{likes}}</span>
        </p>
        <p class="info-item">
            <b>Views </b>
            <span>{{views}}</span>
        </p>
        <p class="info-item">
            <b>Comment </b>
            <span>{{comments}}</span>
        </p>
        <p class="info-item">
            <b>Downloads </b>
            <span>{{downloads}}</span>
        </p>
    </div>
</div>